#!/bin/bash
if [ ! -d ".venv" ] ; then
    python3 -m venv .venv
    source .venv/bin/activate
    #pip3 install --requirement requirements.txt
    pip3 install --requirement test-requirements.txt
else
    source .venv/bin/activate
fi

if [ "${PS1}" != "" ] ; then
    export PS1_ENVRC_PRE="${PS1}"
fi
unset PS1